<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editNama"
        android:layout_width="194dp"
        android:layout_height="42dp"
        android:layout_marginTop="192dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="siap-siap mau pindah.."
        app:layout_constraintBottom_toTopOf="@+id/viewYa"
        app:layout_constraintStart_toStartOf="@+id/viewYa"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/viewYa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mana yang akan ditampilkan?"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonPindah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:text="Pindah Activity"
        android:onClick="pindah"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewYa" />

</androidx.constraintlayout.widget.ConstraintLayout>
